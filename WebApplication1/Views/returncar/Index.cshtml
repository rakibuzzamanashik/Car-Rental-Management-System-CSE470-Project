<div class="row">

    <div class="row">
        @using (Html.BeginForm("Save", "returncar", FormMethod.Post, new { id = "popupForm" }))
        {
            <div>
                <h1>Car Rental</h1>
            </div>



            <div class="=" form-group">
                <label class="form-label">Car no</label>
                <input type="text" id="carid" name="carid" class="form-control" placeholder="CarID" required />
                    <option>Please Select</option>
                </select>
            </div>

            <div class="=" form-group">
                <label class="form-label">Customer ID</label>
                <input type="text" id="custid" name="custid" class="form-control" placeholder="CustID" required />
            </div>

            <div class="=" form-group">
                <label class="form-label">Date</label>
                <input type="text" id="date" name="date" class="form-control" placeholder="Date" required />
            </div>



            <div class="=" form-group">
                <label class="form-label">Days Elapsed</label>
                <input type="text" id="elap" name="elap" class="form-control" placeholder="Rental Fee" required />
            </div>

            <div class="=" form-group">
                <label class="form-label">Fine</label>
                <input type="date" id="fine" name="fine" class="form-control" placeholder="Rental Fee" required />
            </div>

            

            <div class="=" form-group">
                <input type="'submit" value="Save" class="btn btn-success" />
            </div>






        }
    </div>

</div>



@section scripts
{
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>

    <script>

        getCarid();
        function getCarid() {

            $("#carid").keyup(function (e) {



                $.ajax({

                    type: 'POST',
                    url: '/return/Getidk?id=' + $("#carid").val(),
                    dataType: 'JSON',
                    success: function (data) {
                        console.log(data);
                        $("custname").val(data)


                    }


                });



            });




        }


    </script>

}